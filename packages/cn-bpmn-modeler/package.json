{
  "name": "cn-bpmn-modeler-vue",
  "version": "1.0.3",
  "main": "index.ts",
  "unpkg": "dist/cn-bpmn-modeler-vue.global.js",
  "jsdelivr": "dist/cn-bpmn-modeler-vue.global.js",
  "module": "dist/cn-bpmn-modeler-vue.mjs",
  "types": "dist/cn-bpmn-modeler-vue.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cn-bpmn-modeler-vue.d.ts",
      "node": {
        "import": {
          "production": "./dist/cn-bpmn-modeler-vue.node.mjs",
          "development": "./dist/cn-bpmn-modeler-vue.node.mjs",
          "default": "./dist/cn-bpmn-modeler-vue.node.mjs"
        },
        "require": {
          "production": "./dist/cn-bpmn-modeler-vue.prod.cjs",
          "development": "./dist/cn-bpmn-modeler-vue.cjs",
          "default": "./index.js"
        }
      },
      "import": "./dist/cn-bpmn-modeler-vue.mjs",
      "require": "./index.js"
    },
    "./dist/*": "./dist/*",
    "./vetur/*": "./vetur/*",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/MCaiNiaoM/cn-bpmn-modeler-vue.git"
  },
  "bugs": {
    "url": "https://gitee.com/MCaiNiaoM/cn-bpmn-modeler-vue/issues"
  },
  "homepage": "https://gitee.com/MCaiNiaoM/cn-bpmn-modeler-vue/blob/main/README.md",
  "scripts": {
    "dev": "jest --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "build:dts": "api-extractor run --local --verbose && tail -n +10 src/globalExtensions.ts >> dist/cn-bpmn-modeler-vue.d.ts",
    "build:playground": "vue-tsc --noEmit && vite build --config playground/vite.config.ts",
    "build:e2e": "vue-tsc --noEmit && vite build --config e2e/vite.config.mjs",
    "build:size": "pnpm run build && rollup -c size-checks/rollup.config.mjs",
    "dev:e2e": "vite --config e2e/vite.config.mjs",
    "test:types": "tsc --build tsconfig.json",
    "test:dts": "tsc -p ./test-dts/tsconfig.json",
    "test:unit": "jest --coverage",
    "test": "pnpm run test:types && pnpm run test:unit && pnpm run build && pnpm run build:dts && pnpm run test:e2e",
    "test:e2e": "pnpm run test:e2e:headless",
    "test:e2e:headless": "node e2e/runner.mjs --env chrome-headless",
    "test:e2e:native": "node e2e/runner.mjs --env chrome",
    "test:e2e:ci": "node e2e/runner.mjs --env chrome-headless --retries 2",
    "test:e2e:bs": "node e2e/runner.mjs --local -e android5 --tag browserstack",
    "test:e2e:bs-test": "node e2e/runner.mjs --local --env browserstack.local_chrome --tag browserstack"
  },
  "dependencies": {
    "@vueuse/core": "^9.12.0",
    "bpmn-js": "^11.3.1",
    "diagram-js": "^11.9.0",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "lint-staged": "^13.1.1",
    "pont-engine": "1.4.0",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx,vue}": [
      "pnpm lint",
      "prettier --write"
    ],
    "src/**/*.{css,less,scss}": [
      "prettier --write"
    ],
    "src/**/*.{md,json}": [
      "prettier --write"
    ]
  }
}
