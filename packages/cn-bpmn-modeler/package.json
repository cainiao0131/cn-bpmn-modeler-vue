{
  "name": "cn-bpmn-modeler-vue",
  "version": "1.0.7",
  "main": "index.ts",
  "module": "index.ts",
  "sideEffects": false,
  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/MCaiNiaoM/cn-bpmn-modeler-vue.git"
  },
  "bugs": {
    "url": "https://gitee.com/MCaiNiaoM/cn-bpmn-modeler-vue/issues"
  },
  "homepage": "https://gitee.com/MCaiNiaoM/cn-bpmn-modeler-vue/blob/main/README.md",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.mjs",
    "dev:e2e": "vite --config e2e/vite.config.mjs",
    "test:types": "tsc --build tsconfig.json",
    "test:dts": "tsc -p ./test-dts/tsconfig.json",
    "test": "pnpm run test:types && pnpm run test:unit && pnpm run build && pnpm run build:dts && pnpm run test:e2e"
  },
  "dependencies": {
    "bpmn-js": "*",
    "diagram-js": "*",
    "vue": "*",
    "@vueuse/core": "*"
  },
  "devDependencies": {
    "vite": "*",
    "vite-plugin-compression": "*",
    "vite-plugin-eslint": "*",
    "vite-plugin-vue-devtools": "*",
    "vue-tsc": "*",
    "eslint": "*",
    "eslint-plugin-prettier": "*",
    "eslint-plugin-vue": "*",
    "lint-staged": "*",
    "@commitlint/cli": "*",
    "@commitlint/config-conventional": "*",
    "@types/node": "*",
    "@vitejs/plugin-vue": "*",
    "@vitejs/plugin-vue-jsx": "*",
    "@vue/eslint-config-prettier": "*",
    "@vue/eslint-config-typescript": "*",
    "prettier": "*",
    "typescript": "*",
    "less": "*",
    "rollup-plugin-visualizer": "*",
    "unplugin-auto-import": "*",
    "unplugin-vue-components": "*"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx,vue}": [
      "pnpm lint",
      "prettier --write"
    ],
    "src/**/*.{css,less,scss}": [
      "prettier --write"
    ],
    "src/**/*.{md,json}": [
      "prettier --write"
    ]
  }
}
